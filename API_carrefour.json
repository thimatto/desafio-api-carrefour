                      "pm.test(\"Response retorna message e authorization\", function () {\r",
                      "    const jsonData = pm.response.json();\r",
                      "    \r",
                      "    pm.expect(jsonData).to.have.property(\"message\");\r",
                      "    pm.expect(jsonData).to.have.property(\"authorization\");\r",
                      "});\r",
                      "\r",
                      "pm.test(\"Armazena token no environment\", function () {\r",
                      "    const jsonData = pm.response.json();\r",
                      "    \r",
                      "    // Salva o token no environment\r",
                      "    pm.environment.set(\"token\", jsonData.authorization);\r",
                      "\r",
                      "    // Valida que o token foi realmente salvo\r",
                      "    pm.expect(pm.environment.get(\"token\")).to.be.a(\"string\").and.not.empty;\r",
