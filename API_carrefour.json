    {
      "name": "Rate Limit",
      "item": [
        {
          "name": "GET /Validando Rate Limit",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "let count = parseInt(pm.environment.get(\"requestCount\"));\r",
                  "let status = pm.response.code;\r",
                  "\r",
                  "// Até 100 deve ser 200\r",
                  "if (count <= 100) {\r",
                  "    pm.test(`Requisição ${count} deve retornar 200`, function () {\r",
                  "        pm.expect(status).to.eql(200);\r",
                  "    });\r",
                  "}\r",
                  "\r",
                  "// Após 101 deve ser 429\r",
                  "if (count > 100) {\r",
                  "    pm.test(`Requisição ${count} deve retornar 429`, function () {\r",
                  "        pm.expect(status).to.eql(429);\r",
                  "    });\r",
                  "}\r",
                  "\r",
                  ""
                ],
                "type": "text/javascript",
                "packages": {},
                "requests": {}
              }
            },
            {
              "listen": "prerequest",
              "script": {
                "exec": [
                  "if (!pm.environment.get(\"requestCount\")) {\r",
                  "    pm.environment.set(\"requestCount\", 1);\r",
                  "} else {\r",
                  "    let count = parseInt(pm.environment.get(\"requestCount\"));\r",
                  "    pm.environment.set(\"requestCount\", count + 1);\r",
                  "}\r",
                  ""
                ],
                "type": "text/javascript",
                "packages": {},
                "requests": {}
              }
            }
          ],
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "{{token}}",
                "type": "text"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/usuarios",
              "host": [
                "{{baseUrl}}"
              ],
              "path": [
                "usuarios"
              ]
            }
          },
          "response": []
        }
